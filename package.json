{
  "name": "restful-service",
  "version": "1.0.0",
  "description": "Restful Service",
  "main": "index.js",
  "repository": "git",
  "author": "Antoni D",
  "license": "MIT",
  "private": true,
  "scripts": {
    "precommit": "pretty-quick --staged",
    "pretest": "eslint src",
    "test": "node -r dotenv/config ./node_modules/.bin/jest",
    "test:coverage": "yarn test --coverage",
    "build": "NODE_ENV=production rimraf dist/ && babel src --out-dir dist --source-maps inline --copy-files",
    "start": "node -r dotenv/config dist/server.js",
    "dev": "NODE_ENV=development node -r dotenv/config ./node_modules/.bin/nodemon src/server.js --watch src --exec babel-node",
    "serve": "NODE_ENV=production node -r dotenv/config ./node_modules/.bin/pm2-runtime start dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "ioredis": "^4.5.1",
    "moment": "^2.22.2",
    "mongoose": "^5.3.4",
    "pm2": "^3.2.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "babel-jest": "^23.6.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^23.0.1",
    "nodemon": "^1.18.5",
    "prettier": "^1.13.0",
    "pretty-quick": "^1.6.0"
  }
}
